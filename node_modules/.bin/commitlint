#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kimminjae/Documents/jeju_deers/node_modules/.pnpm/@commitlint+cli@19.3.0_@types+node@20.12.10_typescript@5.4.5/node_modules/@commitlint/cli/node_modules:/Users/kimminjae/Documents/jeju_deers/node_modules/.pnpm/@commitlint+cli@19.3.0_@types+node@20.12.10_typescript@5.4.5/node_modules/@commitlint/node_modules:/Users/kimminjae/Documents/jeju_deers/node_modules/.pnpm/@commitlint+cli@19.3.0_@types+node@20.12.10_typescript@5.4.5/node_modules:/Users/kimminjae/Documents/jeju_deers/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kimminjae/Documents/jeju_deers/node_modules/.pnpm/@commitlint+cli@19.3.0_@types+node@20.12.10_typescript@5.4.5/node_modules/@commitlint/cli/node_modules:/Users/kimminjae/Documents/jeju_deers/node_modules/.pnpm/@commitlint+cli@19.3.0_@types+node@20.12.10_typescript@5.4.5/node_modules/@commitlint/node_modules:/Users/kimminjae/Documents/jeju_deers/node_modules/.pnpm/@commitlint+cli@19.3.0_@types+node@20.12.10_typescript@5.4.5/node_modules:/Users/kimminjae/Documents/jeju_deers/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@commitlint/cli/cli.js" "$@"
else
  exec node  "$basedir/../@commitlint/cli/cli.js" "$@"
fi
